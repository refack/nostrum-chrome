{
   "background_page": "content/background.html",
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Flash/Silvelight block for Chrome. Whitelist enabled.",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png"
   },

   "name": "NostrumBlock",
   "options_page": "options/options.html",
   "page_action": {
      "default_icon": "icons/blocked.png",
      "popup": "content/popup.html"
   },
   "permissions": [ "tabs", "http://*/*" ],
   "version": "1.1"
}
